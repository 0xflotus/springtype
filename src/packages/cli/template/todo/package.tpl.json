{
  "dependencies": [
    "@springtype/core",
    "@springtype/i18n",
    "@springtype/router",
    "@springtype/state",
    "@springtype/test",
    "materialize-css",
    "testcafe"
  ],
  "devDependencies": [
    "typescript",
    "tslib",
    "@types/node",
    "cross-env",
    "date-fns",
    "mocha",
    "parcel",
    "rimraf",
    "sass",
    "ts-node"
  ],
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "e2e": "testcafe chrome e2e",
    "prune": "npx rimraf dist .cache node_modules package-lock.json",
    "clean": "npx rimraf dist .cache",
    "start": "npx parcel src/index.html --open",
    "serve": "npx parcel serve src/index.html",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --public-url ."
  }
}