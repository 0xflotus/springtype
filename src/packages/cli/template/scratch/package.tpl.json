{
  "dependencies": [
    "@springtype/core"
  ],
  "devDependencies": [
    "cross-env",
    "parcel-bundler",
    "rimraf",
    "typescript"
  ],
  "scripts": {
    "prune": "npx rimraf dist .cache node_modules package-lock.json",
    "clean": "npx rimraf dist .cache",
    "start": "npx parcel src/index.html --open",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --public-url ."
  }

}